@article{Tremoleda2011,
abstract = {Preclinical models for musculoskeletal disorders are critical for understanding the pathogenesis of bone and joint disorders in humans and the development of effective therapies. The assessment of these models primarily relies on morphological analysis which remains time consuming and costly, requiring large numbers of animals to be tested through different stages of the disease. The implementation of preclinical imaging represents a keystone in the refinement of animal models allowing longitudinal studies and enabling a powerful, non-invasive and clinically translatable way for monitoring disease progression in real time. Our aim is to highlight examples that demonstrate the advantages and limitations of different imaging modalities including magnetic resonance imaging (MRI), computed tomography (CT), positron emission tomography (PET), single-photon emission computed tomography (SPECT) and optical imaging. All of which are in current use in preclinical skeletal research. MRI can provide high resolution of soft tissue structures, but imaging requires comparatively long acquisition times; hence, animals require long-term anaesthesia. CT is extensively used in bone and joint disorders providing excellent spatial resolution and good contrast for bone imaging. Despite its excellent structural assessment of mineralized structures, CT does not provide in vivo functional information of ongoing biological processes. Nuclear medicine is a very promising tool for investigating functional and molecular processes in vivo with new tracers becoming available as biomarkers. The combined use of imaging modalities also holds significant potential for the assessment of disease pathogenesis in animal models of musculoskeletal disorders, minimising the use of conventional invasive methods and animal redundancy.},
author = {Tremoleda, Jordi L and Khalil, Magdy and Gompels, Luke L and Wylezinska-Arridge, Marzena and Vincent, Tonia and Gsell, Willy},
doi = {10.1186/2191-219X-1-11},
file = {:Users/bpaniagua/Library/Application Support/Mendeley Desktop/Downloaded/Tremoleda et al. - 2011 - Imaging technologies for preclinical models of bone and joint disorders.pdf:pdf},
issn = {2191-219X},
journal = {EJNMMI research},
month = {jul},
number = {1},
pages = {11},
pmid = {22214535},
publisher = {Springer},
title = {{Imaging technologies for preclinical models of bone and joint disorders.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22214535},
volume = {1},
year = {2011}
}

@article{Miller2016,
abstract = {{\textless}h3{\textgreater}Importance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Additional therapies are needed for prevention of osteoporotic fractures. Abaloparatide is a selective activator of the parathyroid hormone type 1 receptor.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}To determine the efficacy and safety of abaloparatide, 80 $\mu$g, vs placebo for prevention of new vertebral fracture in postmenopausal women at risk of osteoporotic fracture.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Design, Setting, and Participants{\textless}/h3{\textgreater}{\textless}p{\textgreater}The Abaloparatide Comparator Trial In Vertebral Endpoints (ACTIVE) was a phase 3, double-blind, RCT (March 2011-October 2014) at 28 sites in 10 countries. Postmenopausal women with bone mineral density (BMD) T score ≤−2.5 and {\textgreater}−5.0 at the lumbar spine or femoral neck and radiological evidence ≥2 mild or ≥1 moderate lumbar or thoracic vertebral fracture or history of low-trauma nonvertebral fracture within the past 5 years were eligible. Postmenopausal women ({\textgreater}65 y) with fracture criteria and a T score ≤−2.0 and {\textgreater}−5.0 or without fracture criteria and a T score ≤−3.0 and {\textgreater}−5.0 could enroll.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Interventions{\textless}/h3{\textgreater}{\textless}p{\textgreater}Blinded, daily subcutaneous injections of placebo (n = 821); abaloparatide, 80 $\mu$g (n = 824); or open-label teriparatide, 20 $\mu$g (n = 818) for 18 months.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Main Outcomes and Measures{\textless}/h3{\textgreater}{\textless}p{\textgreater}Primary end point was percentage of participants with new vertebral fracture in the abaloparatide vs placebo groups. Sample size was set to detect a 4{\%} difference (57{\%} risk reduction) between treatment groups. Secondary end points included change in BMD at total hip, femoral neck, and lumbar spine in abaloparatide-treated vs placebo participants and time to first incident nonvertebral fracture. Hypercalcemia was a prespecified safety end point in abaloparatide-treated vs teriparatide participants.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Among 2463 women (mean age, 69 years [range, 49-86]), 1901 completed the study. New morphometric vertebral fractures occurred in 0.58{\%} (n = 4) of the abaloparatide group, 4.22{\%} (n = 30) of the placebo group (risk difference [RD] vs placebo, −3.64 [95{\%} CI, −5.42 to −2.10]; relative risk, 0.14 [95{\%} CI, 0.05-0.39];{\textless}i{\textgreater}P{\textless}/i{\textgreater} {\textless} .001), and 0.84{\%} (n = 6) of the teriparatide group. The Kaplan-Meier estimated event rate for nonvertebral fracture was 2.7{\%} for abaloparatide, 4.7{\%} for placebo (RD, −2.01 [95{\%} CI, −4.02 to −0.00]; hazard ratio [HR], 0.57 [95{\%} CI, 0.32-1.00];{\textless}i{\textgreater}P{\textless}/i{\textgreater} = .049), and 3.3{\%} for teriparatide. BMD increases were greater with abaloparatide than with placebo (all{\textless}i{\textgreater}P{\textless}/i{\textgreater} {\textless} .001). Incidence of hypercalcemia was lower with abaloparatide (3.4{\%}) than with teriparatide (6.4{\%}) with an RD of −2.96 (95{\%} CI, −5.12 to −0.87;{\textless}i{\textgreater}P{\textless}/i{\textgreater} = .006).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions and Relevance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Among postmenopausal women with osteoporosis, the use of subcutaneous abaloparatide, compared with placebo, reduced the risk of new vertebral and nonvertebral fractures over 18 months. Further research is needed to understand the clinical importance of RD, the risks and benefits of abaloparatide treatment, and the efficacy of abaloparatide vs other osteoporosis treatments.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Trial Registration{\textless}/h3{\textgreater}{\textless}p{\textgreater}clinicaltrials.gov Identifier:NCT01343004{\textless}/p{\textgreater}},
author = {Miller, Paul D. and Hattersley, Gary and Riis, Bente Juel and Williams, Gregory C. and Lau, Edith and Russo, Luis Augusto and Alexandersen, Peter and Zerbini, Cristiano A. F. and Hu, Ming-yi and Harris, Alan G. and Fitzpatrick, Lorraine A. and Cosman, Felicia and Christiansen, Claus},
doi = {10.1001/jama.2016.11136},
file = {:Users/bpaniagua/Library/Application Support/Mendeley Desktop/Downloaded/Miller et al. - 2016 - Effect of Abaloparatide vs Placebo on New Vertebral Fractures in Postmenopausal Women With Osteoporosis.pdf:pdf},
issn = {0098-7484},
journal = {JAMA},
keywords = {abaloparatide,fracture,hypercalcemia,osteoporosis,postmenopause,spinal fractures,teriparatide,vertebrae},
month = {aug},
number = {7},
pages = {722},
publisher = {American Medical Association},
title = {{Effect of Abaloparatide vs Placebo on New Vertebral Fractures in Postmenopausal Women With Osteoporosis}},
doi = {doi=10.1001/jama.2016.11136},
volume = {316},
year = {2016}
}

