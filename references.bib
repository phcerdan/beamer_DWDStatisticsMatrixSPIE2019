keywords = {Adolescent,Adult,Cephalometry,Cephalometry: instrumentation,Cephalometry: methods,Child,Computer Simulation,Cone-Beam Computed Tomography,Facial Asymmetry,Facial Asymmetry: radiography,Humans,Imaging, Three-Dimensional,Imaging, Three-Dimensional: instrumentation,Imaging, Three-Dimensional: methods,Mandible,Mandible: abnormalities,Mandible: radiography,Mandibular Diseases,Mandibular Diseases: radiography,Models, Anatomic,Orthognathic Surgical Procedures,Orthognathic Surgical Procedures: instrumentation,Skull Base,Skull Base: radiography,Young Adult},
